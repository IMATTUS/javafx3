<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Calculate Sum" />
      <TextField fx:id="txtNumber1" layoutX="15.0" layoutY="42.0" promptText="First Number" />
      <TextField fx:id="txtNumber2" layoutX="15.0" layoutY="77.0" promptText="Second Number" />
      <Button fx:id="btSum" layoutX="15.0" layoutY="125.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Sum" />
      <Label layoutX="14.0" layoutY="171.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="76.0" layoutY="171.0" />
   </children>
</AnchorPane>
